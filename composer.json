{
    "name": "abenevaut/api-oauth2-sdk",
    "description": "api.benevaut.fr OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Antoine Benevaut",
            "email": "antoine@benevaut.fr",
            "homepage": "https://www.benevaut.fr"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication",
        "api.benevaut.fr"
    ],
    "require": {
        "php": ">=7.3",
        "league/oauth2-client": "^2.0"
    },
    "require-dev": {
        "ext-json": "*",
        "phpunit/phpunit": "^9.4",
        "mockery/mockery": "~1.5.1",
        "squizlabs/php_codesniffer": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "League\\OAuth2\\Client\\Grant\\": "src/Grant",
            "abenevaut\\OAuth2\\Provider\\": "src/Provider"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    }
}
